<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-inherit="documents.DocumentsInspector.buttons" t-inherit-mode="extension">
        <xpath expr="//t[@t-if='nbPreviews === 1']" position="inside">
            <button t-if="records[0].data.type !== 'url'" class="o_inspector_button o_inspector_replace btn" title="View Form" t-on-click.stop.prevent="onClickResModel" t-att-disabled="editDisabled || locked">
                <i class="fa fa-file-text-o"/>
            </button>
        </xpath>
        <xpath expr="//t[@t-if='nbPreviews === 1']" position="attributes">
            <attribute name="t-if">nbPreviews === 1 and records[0].data.is_manager</attribute>
        </xpath>

      

        <xpath expr="//t[@t-if='isPdfOnly()']" position="attributes">
            <attribute name="t-if">isPdfOnly() and records[0].data.is_manager</attribute>
        </xpath>
        <xpath expr="//div/button[@t-if='nbPreviews === 1 and !isMobile and !state.showChatter']" position="attributes">
            <attribute name="t-if">nbPreviews === 1 and !isMobile and !state.showChatter and records[0].data.is_manager</attribute>
        </xpath>
    </t>
    <t t-inherit="documents.DocumentsInspector.documentsInfo" t-inherit-mode="extension">
