<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="config_parameter_evaluation_system" model="ir.config_parameter">
            <field name="key">oi_risk_management.evaluation_system</field>
            <field name="value">symmetric</field>
        </record>
        <record id="config_parameter_asymmetric_evaluation_very_high_ids" model="ir.config_parameter">
            <field name="key">oi_risk.asymmetric_evaluation_very_high_ids</field>
            <field name="value">P2 S5,P3 S4,P3 S5,P4 S4,P4 S5,P5 S3,P5 S4,P5 S5</field>
        </record>
        <record id="config_parameter_asymmetric_evaluation_high_ids" model="ir.config_parameter">
            <field name="key">oi_risk.asymmetric_evaluation_high_ids</field>
            <field name="value">P1 S4,P1 S5,P2 S4,P3 S3,P4 S2,P4 S3,P5 S2</field>
        </record>
        <record id="config_parameter_asymmetric_evaluation_medium_ids" model="ir.config_parameter">
            <field name="key">oi_risk.asymmetric_evaluation_medium_ids</field>
            <field name="value">P1 S3,P2 S3,P3 S2,P4 S1,P5 S1</field>
        </record>
        <record id="config_parameter_asymmetric_evaluation_low_ids" model="ir.config_parameter">
            <field name="key">oi_risk.asymmetric_evaluation_low_ids</field>
            <field name="value">P1 S1,P1 S2,P2 S1,P2 S2,P3 S1</field>
        </record>
        <record id="config_parameter_do_risk_treatments_have_owner" model="ir.config_parameter">
            <field name="key">oi_risk_management.do_risk_treatments_have_owner</field>
            <field name="value" eval="True"></field>
        </record>
        <record id="config_parameter_main_risk_type" model="ir.config_parameter">
            <field name="key">oi_risk_management.main_risk_type</field>
            <field name="value">current_risk</field>
        </record>
    </data>
</odoo>
