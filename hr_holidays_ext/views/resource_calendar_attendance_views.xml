<?xml version='1.0' encoding='UTF-8' ?>
<odoo>

    <record id="view_resource_calendar_attendance_est_tree" model="ir.ui.view">
        <field name="name">resource.calendar.attendance.ext.tree</field>
        <field name="model">resource.calendar.attendance</field>
        <field name="inherit_id" ref="resource.view_resource_calendar_attendance_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='duration_days']" position="after">
                <field name="week_end"/>
            </xpath>
        </field>
    </record>

</odoo>
