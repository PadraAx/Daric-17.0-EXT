<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="ProductDashboardABC">
        <div class="appointment_dashboard">
            <div class="container-fluid o_appointment_dashboard">
                <div class="sales-dashboard">
                  <h4 class="text-primary text-center py-5">ProductABC</h4>
                    <div class="container">
                    <div class="appointment-cards pb-5">

<!--                       Start: Appointment Charts-->
                        <div class="row">

                        <div class="col-md-12">
                          <div class="appontment_chart_cards p-sm-3 p-2 shadow-sm text-center border rounded">
                            <h4 class="mb-2 text-dark">Product Bar</h4>
                              <div class="card-body shadow">
                              <div class="panel">
                                  <div class="panel-heading">
                                  </div>
                                  <div class="panel-body">
                                      <canvas id="highprice" class="header-box-p"/>
                                  </div>
                              </div>
                            </div>
                          </div>
                        </div>


                            <div class="col-md-12">
                          <div class="appontment_chart_cards p-sm-3 p-2 shadow-sm text-center border rounded">
                            <h4 class="mb-2 text-dark">Product Line</h4>
                              <div class="card-body shadow">
                              <div class="panel">
                                  <div class="panel-heading">
                                  </div>
                                  <div class="panel-body">
                                          <canvas id="sale_order_generated" class="header-box-p"/>
                                    </div>
                              </div>
                            </div>
                          </div>
                        </div>

                      </div>
                        <!-- End: Appointment Charts -->
                    </div>
                  </div>
                </div>
              </div>
          </div>
      </t>
</templates>
