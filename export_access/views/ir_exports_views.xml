<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="ir_exports_view_tree" model="ir.ui.view">
        <field name="name">ir.exports.view.tree</field>
        <field name="model">ir.exports</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name"/>
                <field name="resource"/>
            </tree>
        </field>
    </record>

    <record id="ir_exports_form" model="ir.ui.view">
        <field name="name">ir.exports.form</field>
        <field name="model">ir.exports</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="resource"/>
                    </group>
                    <notebook>
                        <page name="export_fields" string="Export Fields">
                            <field name="export_fields"/>
                        </page>
                        <page name="user_ids" string="Users">
                            <field name="user_ids"/>
                        </page>
                        <page name="group_ids" string="Groups">
                            <field name="group_ids"/>
                        </page>
                    </notebook>
                    
                </sheet>
            </form>
        </field>
    </record>

    <record id="ir_exports_action" model="ir.actions.act_window">
        <field name="name">Export Access</field>
        <field name="res_model">ir.exports</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>